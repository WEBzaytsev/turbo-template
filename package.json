{
  "devDependencies": {
    "knip": "^5.70.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.6",
    "prettier-plugin-sh": "^0.15.0",
    "prettier-plugin-sort-json": "^4.0.0",
    "prettier-plugin-sql": "^0.19.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "rimraf": "^6.0.1",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=20"
  },
  "name": "turbo-template-zbn",
  "packageManager": "npm@11.5.2",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:api": "turbo run dev --filter=api",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "prelint:fix": "npm run format",
    "prelint": "npm run format",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,scss,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,scss,yaml,yml}\"",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "test:cov": "turbo run test:cov",
    "knip": "knip --no-exit-code",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "clean:turbo": "rm -rf .turbo && rm -rf apps/*/.turbo && rm -rf packages/*/.turbo"
  },
  "workspaces": ["apps/*", "packages/*"]
}
